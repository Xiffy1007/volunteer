  overflow: hidden;
  position: relative;
  background-color: var(--themeColor);
}
.user_info{
  position:absolute;
  padding: 0 20rpx ;
  top: 20%;
  text-align: center;
  display: flex;

}
.user_icon{
  width: 125rpx;
  height: 125rpx;
  border-radius: 50%;
}
.user_name{
  padding-left: 20rpx;
  color: white;
 margin-top: 40rpx;
}
